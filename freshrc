#!/bin/zsh
# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions/\*
#   fresh jasoncodes/dotfiles shell/aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin
fresh freshshell/fresh contrib/source-build.sh --file=~/.zshrc

fresh zshrc
fresh powerlevel9k
fresh zplug
fresh aliases
fresh functions
fresh fzf.zsh
fresh lessrc
fresh https://gitlab.com/ellsclytn/dotfiles.git bin/extract --bin --ref=5165c8d0

# Git
fresh-options --file
  fresh git/\*
fresh-options

fresh config/Karabiner/karabiner.json --file=~/.config/Karabiner/karabiner.json

# fresh profile
fresh pryrc --file=~/.pryrc
fresh gemrc --file=~/.gemrc
fresh railsrc --file=~/.railsrc
